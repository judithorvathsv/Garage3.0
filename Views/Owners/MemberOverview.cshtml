@model IEnumerable<Garage3.Models.ViewModels.MemberDetailsViewModel>

@{
    ViewData["Title"] = "MemberOverview";
}

<h1>Members Overview</h1>

<table class="table">

    <thead>
    <tr>
            <th> @Html.DisplayNameFor(model => model.FullName)        </th>

            <th> @Html.DisplayNameFor(model => model.NumberOfVehicles)        </th>
    </tr>
    <thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>

                <td> @Html.DisplayFor(modelItem => item.FullName)  </td>
                <td> @Html.DisplayFor(modelItem => item.NumberOfVehicles)  </td>

            <td>
                <a class="btn btn-primary" asp-action=" ">Member view</a>
            </td>
        </tr>
    }
    </tbody>
</table>

